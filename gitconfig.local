# These are settings useful in this repository. Add them to your clone's
# config. (The .git/config file cannot be shared within the repository, so it's
# up to the cloner to get it.)

[alias]
	subaddvim = "!f(){ [ $# -eq 2 ] && git submodule add $1 multi/vim/bundle/$2 ; };f"
	pullmasterrebase = "!f(){ [ $# -eq 1 ] && git checkout master && git pull --rebase github master && git checkout $1 && git rebase master ; };f"

